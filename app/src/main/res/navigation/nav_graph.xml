<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.ca1.BlankFragment"
        android:label="blank_fragment"
        tools:layout="@layout/blank_fragment" >
        <action
            android:id="@+id/action_blankFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <argument
            android:name="info"
            app:argType="com.example.ca1.DataResult" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.ca1.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>